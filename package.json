{
	"name": "vroot",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"pretty": "prettier --write \"./**/*.{ts,scss,css,html,json}\"",
		"extract-i18n": "ngx-translate-extract  -i ./src -o ./public/i18n/en.json -o ./public/i18n/ar.json -f namespaced-json",
		"//1": "###################################################################",
		"//2": "vroot",
		"//3": "########",
		"vroot": "ng serve vroot",
		"serve-vroot": "ng serve vroot --configuration development",
		"build:vroot": "ng build vroot",
		"postbuild:vroot": "scripts\\post-build-vroot.cmd",
		"build:local-vroot": "ng build vroot --base-href /vroot/",
		"postbuild:local-vroot": "scripts\\post-build-local-vroot.cmd",
		"build:all-vroot": "clear; git pull; npm run build:vroot; npm run build:local-vroot",
		"//4": "###################################################################",
		"//5": "webstore",
		"//6": "########",
		"serve-webstore": "ng serve webstore --configuration development",
		"build:webstore": "ng build webstore",
		"postbuild:webstore": "scripts\\post-build-webstore.cmd",
		"build:local-webstore": "ng build webstore --base-href /webstore/",
		"postbuild:local-webstore": "scripts\\post-build-local-webstore.cmd",
		"build:all-webstore": "clear; git pull; npm run build:webstore; npm run build:local-webstore",
		"//7": "###################################################################",
		"//8": "webstore",
		"//9": "########"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^19.1.1",
		"@angular/common": "^19.1.1",
		"@angular/compiler": "^19.1.1",
		"@angular/core": "^19.1.1",
		"@angular/forms": "^19.1.1",
		"@angular/platform-browser": "^19.1.1",
		"@angular/platform-browser-dynamic": "^19.1.1",
		"@angular/router": "^19.1.1",
		"@ngrx/operators": "^19.0.0",
		"@ngrx/signals": "^19.0.0",
		"@ngx-translate/core": "^16.0.4",
		"@primeng/themes": "^19.0.5",
		"@types/google-one-tap": "^1.2.6",
		"google-one-tap": "^1.0.6",
		"primeicons": "^7.0.0",
		"primeng": "^19.0.5",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.1.2",
		"@angular-devkit/core": "^19.1.2",
		"@angular/cli": "^19.1.2",
		"@angular/compiler-cli": "^19.1.1",
		"@types/jasmine": "~5.1.5",
		"@vendure/ngx-translate-extract": "^9.4.0",
		"autoprefixer": "^10.4.20",
		"jasmine-core": "~5.5.0",
		"karma": "~6.4.4",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"postcss": "^8.5.1",
